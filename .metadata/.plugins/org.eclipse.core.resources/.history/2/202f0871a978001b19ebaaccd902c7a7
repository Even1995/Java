import java.util.Scanner;

public class four {

	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		 
		 int n = sc.nextInt();
		 
		 for (int i=0; i <= n; i++) {
			 if (i / 2 != 0) {
				  
				 int a = sc.nextInt();
				 int b = sc.nextInt();
				 
				 double divisao = a/b;
						
				 
					System.out.printf("%.1f%n", divisao);
			 }
		 }
				 
		 sc.close();

	}

}
